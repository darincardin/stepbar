{
  "author": "",
  "dependencies": {
    "@testing-library/react": "^10.1.0",
    "bootstrap": "3.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-15": "^1.4.1",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest-dom": "^4.0.0",
    "jquery": "3.4.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.13.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(scss|css|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "description": "Stepbar using ReactJS and Webpack",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^5.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "css-loader": "^3.5.2",
    "file-loader": "^5.1.0",
    "jest": "^26.0.1",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.13.1",
    "react": "^16.8.4",
    "react-dom": "^16.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.1.1"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "name": "form",
  "scripts": {
    "build": "clear && jest  && webpack --env.production=false --mode development --config webpack.prod.js",
    "start": "clear && webpack --env.production=false --mode development --config webpack.config.js &&  webpack-dev-server --hot --env.production=false",
    "test": "clear && jest"
  },
  "version": "1.0.0"
}
